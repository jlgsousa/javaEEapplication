package studentejb;

import javax.ejb.Stateless;

/**
 * Session Bean implementation class AddBean
 */
@Stateless
public class AddBean implements AddBeanRemote {

    /**
     * Default constructor.
     */
    public AddBean() {
    }

    @Override
    public int add(int a, int b) {
        return a + b;
    }
}
